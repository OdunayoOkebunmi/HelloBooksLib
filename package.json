{
  "name": "hellobookslib",
  "version": "1.0.0",
  "description": "Deferral poject",
  "main": "index.js",
  "scripts": {
    "start": "node build/index.js",
    "test": "jest --forceExit --verbose",
    "coverage": "jest --forceExit --coverage && cat ./coverage/lcov.info | coveralls",
    "dev": "nodemon --exec babel-node server/index.js",
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel -d ./build ./server",
    "build": "npm run clean && npm run build-server",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "migrate:undo": "node_modules/.bin/sequelize db:migrate:undo",
    "seed": "node_modules/.bin/sequelize db:seed:all",
    "seed:undo": "node_modules/.bin/sequelize db:seed:undo",
    "generate:model": "node_modules/.bin/sequelize model:generate",
    "heroku-postbuild": "npm run migrate && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emp-daisy/HelloBooksLib.git"
  },
  "keywords": [
    "Library",
    "Books",
    "Deferral"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/emp-daisy/HelloBooksLib/issues"
  },
  "homepage": "https://github.com/emp-daisy/HelloBooksLib#readme",
  "dependencies": {
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^5.8.6",
    "sequelize-cli": "^5.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "coveralls": "^3.0.3",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.3",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.17.1",
    "supertest": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
