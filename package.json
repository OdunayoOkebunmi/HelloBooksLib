{
  "name": "hellobookslib",
  "version": "1.0.0",
  "description": "Deferral poject",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec babel-node index.js",
    "test": "jest --forceExit --verbose",
    "coverage": "jest --forceExit --coverage && cat ./coverage/lcov.info | coveralls",
    "dev": "nodemon --exec babel-node server/index.js",
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel -d ./build ./server",
    "build": "npm run clean && npm run build-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emp-daisy/HelloBooksLib.git"
  },
  "keywords": [
    "Library",
    "Books",
    "Deferral"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/emp-daisy/HelloBooksLib/issues"
  },
  "homepage": "https://github.com/emp-daisy/HelloBooksLib#readme",
  "dependencies": {
    "dotenv": "^8.0.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.3",
    "dotenv": "^8.0.0",
    "eslint-config-prettier": "^4.3.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.17.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
